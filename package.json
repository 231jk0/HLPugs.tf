{
	"name": "hlpugs-server",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "npm run serve",
		"build": "npm run build-ts && npm run tslint",
		"serve": "node dist/server.js",
		"watch-node": "nodemon dist/server.js",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"tslint": "tslint -c tslint.json -p tsconfig.json"
	},
	"dependencies": {
		"express": "^4.16.3"
	},
	"devDependencies": {
		"@types/express": "^4.16.0",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2"
	}
}
