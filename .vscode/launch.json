{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Intel",
      "program": "${workspaceFolder}/Intel/src/server.ts",
      "preLaunchTask": "npm: build - Intel",
      "cwd": "${workspaceFolder}/Intel",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
        "${workspaceFolder}/Intel/dist/Intel/src/**/*.js"
      ]
    }
  ]
}
